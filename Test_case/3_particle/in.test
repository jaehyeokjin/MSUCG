units           real
atom_style      atomic
newton		off

pair_style      msucg_neigh 10.1 3.0 3.0
pair_modify     mix arithmetic

read_data	p3.data

neighbor        2.0 bin
neigh_modify    every 1
neigh_modify    one 50000
neigh_modify    page 2500000

timestep        0.25

velocity all create 100.0 4928459 rot yes dist gaussian
fix             nvt all nvt temp 100.0 100.0 1.0
compute         3 all coord/atom 4.0

compute         K all ke/atom
compute         P all pe/atom

thermo 1
thermo_style    custom step epair ke pe etotal temp press
dump            1 all custom 1 test.lammpstrj id type xu yu zu vx vy vz c_K c_P c_3
dump_modify     1 format "%d %d %2.10g %2.10g %2.10g %0.0g %0.0g %0.0g %0.0g %2.10g %g"
run             1

